import os

SRC_FOLDER = 'me_project'
DST_FOLDER = 'templates'

dirs = os.walk(SRC_FOLDER)
for x, y, _ in dirs:
    for i in y:
        newpath = os.path.join(os.curdir, DST_FOLDER, os.sep.join(x.split(os.sep)[1:]),i)
        print(x.split(os.sep))
        os.makedirs(newpath)
        
